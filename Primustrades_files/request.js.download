        loadInvoice: function(id) {
            var btn = id;
            //console.log(btn);
            $.each(btn,function(index,elem) {
                elem.onclick = function() {
                    
                    var requestId = elem.dataset.requestId;
                    var name = elem.dataset.name;
                    request.setInvoice(requestId,name);
                    
                };
            });
            
        },
    
